{
  "name": "return-youtube-dislike",
  "version": "2.0.0",
  "description": "Chrome extension to return youtube dislikes",
  "main": "index.js",
  "scripts": {
    "web-run": "npm run web-run:chrome",
    "web-run:firefox": "npm run build:dev:firefox && cd dist && web-ext run --start-url https://addons.mozilla.org/firefox/addon/ublock-origin/",
    "web-run:chrome": "npm run build:dev:chrome && cd dist && web-ext run --start-url https://chrome.google.com/webstore/detail/ublock-origin/cjpalhdlnbpafiamejdnhcphjbkeiagm -t chromium",
    "build": "npm run build:chrome",
    "build:chrome": "rimraf dist && webpack --env browser=chrome --config webpack/webpack.prod.js",
    "build:firefox": "rimraf dist && webpack --env browser=firefox --config webpack/webpack.prod.js",
    "build:dev": "npm run build:dev:chrome",
    "build:dev:chrome": "rimraf dist && webpack --env browser=chrome --config webpack/webpack.dev.js",
    "build:dev:firefox": "rimraf dist && webpack --env browser=firefox --config webpack/webpack.dev.js",
    "build:watch": "npm run build:watch:chrome",
    "build:watch:chrome": "webpack --env browser=chrome --config webpack/webpack.dev.js --watch",
    "build:watch:firefox": "webpack --env browser=firefox --config webpack/webpack.dev.js --watch",
    "dev": "npm run build:dev && concurrently \"npm run web-run\" \"npm run build:watch\"",
    "dev:firefox": "npm run build:dev:firefox && concurrently \"npm run web-run:firefox\" \"npm run build:watch:firefox\"",
    "clean": "rimraf dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.171",
    "@types/firefox-webext-browser": "^94.0.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^6.5.1",
    "copy-webpack-plugin": "^10.2.0",
    "html-webpack-plugin": "^5.5.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "web-ext": "^6.6.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  }
}
